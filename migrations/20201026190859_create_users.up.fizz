create_table("users") {
	t.Column("id", "uuid", {primary: true})
	t.Column("name", "string", {})
	t.Column("email", "string", {null: true})
	t.Column("provider", "string", {})
	t.Column("provider_id", "string", {})
	t.Column("avatar_url", "string", {})
	t.Column("password_hash", "string", {})
	t.Timestamps()
}